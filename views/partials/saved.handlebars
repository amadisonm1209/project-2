    {{#if user.toTry}}
    <ul class="collection">
        {{#each user.toTry}}
        <li class="collection-item">
            <h5>{{this.distillery}}</h5>
            <p><a href="{{this.website}}">Website</a></p>
            <p>{{this.city}}</p>
            <button class="btn waves-effect waves-light remove-saved" data-distid="{{this.id}}">Remove From Saves</button>
            </li>
        {{/each}}
    </ul>
    {{/if}}